generator photon {
  provider = "photonjs"
}

datasource db {
  provider = "mysql"
  url      = env("MYSQL_URL")
}

model Asset {
  id              String   @default(uuid()) @id
  currency        Int
  stockSpot       Int
  investmentTrust Int
  pension         Int
  point           Int
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt

  @@index([updatedAt], name: "idx_asset_updated_at")
}
