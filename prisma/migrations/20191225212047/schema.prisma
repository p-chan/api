generator photon {
  provider = "photonjs"
}

datasource db {
  provider = "mysql"
  url = "***"
}

model Assets {
  id              String   @default(uuid()) @id
  currency        Int
  stockSpot       Int
  investmentTrust Int
  pension         Int
  point           Int
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
}
